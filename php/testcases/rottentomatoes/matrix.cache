<script type="text/javascript">
function reveal(imgId) {
	$('ul.bullets li').each(function(index, element) {
		var e = $(element);
		if (e.hasClass(imgId)) e.addClass('selected');
		else e.removeClass('selected');
	});
	$('div.screenshots img').each(function(index, element) {
		var e = $(element);
		if (e.hasClass(imgId)) e.show();
		else e.hide();
	});
}
